<% layout('layouts/boilerplate')%>


<h1>Booking History</h1>



<% for (let i=0;i<books.length;i++){%>
<div class="card card shadow" mb-3>
    <div class="row"> 
        <div class="col-md-4">
            
           
            <img class="img-fluid" alt="" src="<%=camps[i].url%>">
            
            
        </div>
        <div class="col-md-8">
            <div class="card-body">
                <h5 class="card-title">Booked Campground: <%= camps[i].title %> </h5>
                <p class="card-text">EntryDate :<%= books[i].EntryDate %></p>
                <p class="card-text">ExitDate :<%= books[i].ExitDate %></p>
                <p class="card-text">
                    <small class="text-muted">TotalBill :$<%= books[i].TotalBill %></small>
                </p>
              
            </div>
        </div>
    </div>
</div>
<% }%>
